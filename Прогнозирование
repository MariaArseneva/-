    "import csv\n",
    "road = str(input('Введите путь к файлу (без кавычек) '))\n",
    "with open(road, encoding='utf-8') as r_file:\n",
    "    file_reader = csv.reader(r_file, delimiter = \";\",quoting=csv.QUOTE_NONE)\n",
    "    l=[]\n",
    "    for i in file_reader:\n",
    "        #print(i)\n",
    "        l.append(i)\n",
    "for i in l:\n",
    "    i[0]=int(i[0])\n",
    "    i[1]=int(i[1])\n",
    "print(l)\n",
    "    "
